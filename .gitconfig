# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Jake Nelson
	email = j.n@live.com.au
[init]
	defaultBranch = main
[push]
	followTags = true
[pull]
	rebase = false
[credential]
	helper = osxkeychain
[core]
	editor = code --wait
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[help]
	autocorrect = 10
[rerere]
	enabled = false
[diff]
	algorithm = histogram
[merge]
	conflictstyle = zdiff3
