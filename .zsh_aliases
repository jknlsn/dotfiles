alias ejectall="osascript -e 'tell application \"Finder\" to eject (every disk whose ejectable is true)'"
alias wip="git push --set-upstream origin $branch -o merge_request.create -o merge_request.remove_source_branch -o merge_request.title=\"WIP: Merge $branch\""
alias rev="git push -o merge_request.title=\"Merge $branch\""